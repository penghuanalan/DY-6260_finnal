<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_image"
    tools:context="${relativePackage}.${activityClass}" >

    <TextView 
        android:id="@+id/tv_sample_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
       android:layout_marginLeft="418dp"
        android:text="四 环 素"
        android:textSize="31sp"
        android:textColor="#ffffff"/>
    <TextView 
       android:id="@+id/tv_counter"
       android:layout_width="190dp"
       android:layout_height="wrap_content"
       android:layout_marginTop="20dp"
       android:layout_marginLeft="814dp"
       android:background="@null"
       android:text="00:00:01"
       android:textSize="20dp"/>
     <LinearLayout 
       android:id="@+id/ll_map"
       android:layout_width="256dp"
       android:layout_height="444dp"
       android:orientation="vertical"
       android:layout_marginTop="80dp"
       android:layout_marginLeft="768dp"
       >
      <Button
          android:id="@+id/btn_sample"
          android:layout_width="136dp"
          android:layout_height="50dp"
          android:layout_marginTop="60dp"
         android:layout_marginLeft="60dp"
         android:background="@drawable/sample"
          />
      <Button 
          android:id="@+id/btn_line_01"
          android:layout_width="7dp"
          android:layout_height="40dp"
          android:background="@drawable/line_hui"
          android:layout_marginLeft="124dp"
          /> 
     <pl.droidsonroids.gif.GifImageView 
          android:id="@+id/gif_input_card"
          android:layout_width="136dp"
          android:layout_height="50dp"
          android:background="@drawable/put_the_card"
         android:layout_marginLeft="60dp"
          /> 
          <Button 
          android:id="@+id/btn_line_02"
          android:layout_width="7dp"
          android:layout_height="40dp"
          android:background="@drawable/selector_line"
          android:layout_marginLeft="124dp"
          /> 
      <pl.droidsonroids.gif.GifImageView
          android:id="@+id/gif_loading" 
          android:layout_width="136dp"
          android:layout_height="50dp"
          android:background="@drawable/loading"
         android:layout_marginLeft="60dp"
          /> 
          <Button 
          android:id="@+id/btn_line_03"
          android:layout_width="7dp"
          android:layout_height="40dp"
          android:background="@drawable/selector_line"
          android:layout_marginLeft="124dp"
          /> 
          <Button
              android:id="@+id/btn_complete" 
          android:layout_width="136dp"
          android:layout_height="50dp"
          android:background="@drawable/selector_complete"
         android:layout_marginLeft="60dp"
          /> 
   </LinearLayout>
   <LinearLayout 
       android:id="@+id/ll_channel_bar"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginTop="110dp"
       android:layout_marginLeft="290dp"
       android:background="@drawable/channel_bar_dingliang"
       android:orientation="vertical"
       >
       <TextView 
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:text="通道2"
           android:textSize="26sp"
           android:layout_weight="1"
           android:textColor="#2e2e2e"
           android:paddingLeft="127dp"
           android:paddingTop="4dp"/>
       <RelativeLayout 
           android:layout_width="match_parent"
           android:layout_height="135dp"
           android:textSize="20sp">
            <pl.droidsonroids.gif.GifImageView
                android:id="@+id/gif_data_loading"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"/>
       </RelativeLayout>
       <LinearLayout 
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:orientation="horizontal">
           
       
          <TextView 
               android:layout_width="70dp"
               android:layout_height="match_parent"
               android:text="样品"
               android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="16dp"
               android:paddingTop="4dp"/>
           <EditText
               android:id="@+id/et_input_sample" 
               android:layout_width="249dp"
               android:layout_height="match_parent"
                android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="24dp"
               android:paddingBottom="6dp"
               android:background="@null"
               android:hint="请输入检测样品"/> 
       </LinearLayout>
       <LinearLayout 
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:orientation="horizontal"
          >
          <TextView 
               android:layout_width="70dp"
               android:layout_height="match_parent"
               android:text="编号"
               android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="16dp"
               android:paddingTop="5dp"/>
           <TextView
               android:id="@+id/tv_id_number" 
               android:layout_width="249dp"
               android:layout_height="match_parent"
                android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="24dp"
               android:paddingTop="5dp"
               android:background="@null"
               android:text="0001"/> 
       </LinearLayout>
       <LinearLayout 
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:orientation="horizontal"
          >
          <TextView 
               android:layout_width="70dp"
               android:layout_height="match_parent"
               android:text="倍数"
               android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="16dp"
               android:paddingTop="5dp"
               android:background="@null"
               android:hint="请输入倍数"/> 
           <EditText
               android:id="@+id/tv_input_multiple" 
               android:layout_width="249dp"
               android:layout_height="match_parent"
                android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="24dp"
               android:paddingBottom="6dp"
               android:background="@null"
                android:text="5"/> 
       </LinearLayout>
       <LinearLayout 
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:orientation="horizontal">
          <TextView 
               android:layout_width="70dp"
               android:layout_height="match_parent"
               android:text="浓度"
               android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="16dp"
               android:paddingTop="5dp"/>
           <TextView 
               android:layout_width="250dp"
               android:layout_height="match_parent"
                android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="24dp"
               android:paddingTop="5dp"
               />  
       </LinearLayout>
       <LinearLayout 
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:orientation="horizontal">
           <TextView 
               android:layout_width="70dp"
               android:layout_height="match_parent"
               android:text="结果"
               android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="16dp"
               android:paddingTop="5dp"/>
           <TextView 
               android:layout_width="250dp"
               android:layout_height="match_parent"
                android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="24dp"
               android:paddingTop="5dp"
               /> 
       </LinearLayout>
       
   </LinearLayout>
    <LinearLayout 
       android:id="@+id/ll_channel_complete"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginTop="110dp"
       android:layout_toRightOf="@+id/ll_channel_bar"
       android:background="@drawable/channel_bar_dingliang_wancheng"
       android:layout_marginLeft="290dp"
       android:orientation="vertical"
       android:visibility="gone"
       >
       <TextView 
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:text="通道2"
           android:textSize="26sp"
           android:layout_weight="1"
           android:textColor="#2e2e2e"
           android:paddingLeft="127dp"
           android:paddingTop="4dp"/>
       <RelativeLayout 
           android:layout_width="match_parent"
           android:layout_height="135dp"
           android:textSize="20sp">
            <RelativeLayout
                android:id="@+id/rl_customView"
                android:layout_width="161dp"
                android:layout_height="135dp" >

                <com.dayuan.dy_6260chartscanner.CustomView
                    android:id="@+id/customView"
                    android:layout_width="161dp"
                    android:layout_height="135dp" />
            </RelativeLayout>
            <RelativeLayout 
                android:layout_width="135dp"
                android:layout_height="match_parent"
                android:layout_toRightOf="@+id/rl_customView"
                android:layout_marginLeft="38dp">
                <TextView 
                    android:id="@+id/tv_c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="C值:"
                    android:textSize="20sp"
                    android:layout_marginTop="40dp"/>
                <TextView 
                    android:id="@+id/tv_value_c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/tv_c"
                    android:text="2543"
                    android:textSize="20sp"
                    android:layout_alignBottom="@+id/tv_c"/>
                <TextView 
                    android:id="@+id/tv_t"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="T值:"
                    android:textSize="20sp"
                    android:layout_below="@+id/tv_c"
                    android:layout_marginTop="15dp"/>
                <TextView 
                    android:id="@+id/tv_value_t"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/tv_c"
                    android:text="1435"
                    android:textSize="20sp"
                    android:layout_alignBottom="@+id/tv_t"/>
            </RelativeLayout>
       </RelativeLayout>
       
       <LinearLayout 
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:orientation="horizontal"
          >
          <TextView 
               android:layout_width="70dp"
               android:layout_height="match_parent"
               android:text="样品"
               android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="16dp"
               android:paddingTop="4dp"/>
           <TextView 
               android:id="@+id/tv_sample" 
               android:layout_width="249dp"
               android:layout_height="match_parent"
                android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="24dp"
               android:paddingTop="4dp"
               android:background="@null"
               android:text="猪肉"/> 
       </LinearLayout>
        <LinearLayout 
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:orientation="horizontal"
          >
          <TextView 
               android:layout_width="70dp"
               android:layout_height="match_parent"
               android:text="编号"
               android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="16dp"
               android:paddingTop="5dp"/>
           <TextView
               android:id="@+id/tv_number" 
               android:layout_width="249dp"
               android:layout_height="match_parent"
                android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="24dp"
               android:paddingTop="5dp"
               android:background="@null"
               android:text="0001"/> 
       </LinearLayout>
       <LinearLayout 
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:orientation="horizontal"
          >
          <TextView 
               android:layout_width="70dp"
               android:layout_height="match_parent"
               android:text="倍数"
               android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="16dp"
               android:paddingTop="5dp"/>
           <TextView
               android:id="@+id/tv_multiple" 
               android:layout_width="249dp"
               android:layout_height="match_parent"
                android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="24dp"
               android:paddingTop="5dp"
               android:background="@null"
                android:text=""/> 
       </LinearLayout>
       <LinearLayout 
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:orientation="horizontal">
          <TextView 
               android:layout_width="70dp"
               android:layout_height="match_parent"
               android:text="浓度"
               android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="16dp"
               android:paddingTop="5dp"/>
           <TextView 
               android:layout_width="250dp"
               android:layout_height="match_parent"
                android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="24dp"
               android:paddingTop="5dp"
               />  
       </LinearLayout>
       <LinearLayout 
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:orientation="horizontal">
           <TextView 
               android:layout_width="70dp"
               android:layout_height="match_parent"
               android:text="结果"
               android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="16dp"
               android:paddingTop="5dp"/>
           <TextView 
               android:layout_width="250dp"
               android:layout_height="match_parent"
                android:textSize="18sp"
               android:textColor="#333333"
               android:paddingLeft="24dp"
               android:paddingTop="5dp"
               /> 
       </LinearLayout>
       
   </LinearLayout>
    <Button
             android:id="@+id/btn_generate_report"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_marginLeft="328dp"
             android:layout_below="@+id/ll_map"
             android:onClick="doClick"
             android:background="@drawable/generate_a_report" 
             android:visibility="gone"/>
      <Button
             android:id="@+id/btn_print"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_marginLeft="518dp"
             android:layout_below="@+id/ll_map"
             android:onClick="doClick"
             android:background="@drawable/print"
             android:visibility="gone" />
         <Button
             android:id="@+id/btn_upload"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_marginLeft="654dp"
             android:onClick="doClick"
             android:layout_below="@+id/ll_map"
             android:background="@drawable/upload"
             android:visibility="gone" />
         <Button 
             android:id="@+id/btn_start_test"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_marginLeft="602dp"
             android:onClick="doClick"
             android:background="@drawable/starting_test"
             android:layout_below="@+id/ll_map"/>
         <Button 
             android:id="@+id/btn_return"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_marginLeft="794dp"
             android:layout_marginRight="40dp"
             android:onClick="doClick"
              android:layout_below="@+id/ll_map"
             android:background="@drawable/back"
             />
</RelativeLayout>
