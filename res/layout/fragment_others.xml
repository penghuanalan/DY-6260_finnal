<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/tv_print_setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="186dp"
        android:layout_marginTop="10dp"
        android:gravity="center_vertical"
        android:text="打印设置"
        android:textColor="#3d3d3d"
        android:textSize="18sp" />

    <View
        android:id="@+id/v_line_01"
        android:layout_width="440dp"
        android:layout_height="1dp"
        android:layout_below="@+id/tv_print_setting"
        android:layout_marginLeft="186dp"
        android:layout_marginTop="10dp"
        android:background="#c0c0c0" />

    <LinearLayout
        android:id="@+id/ll_others"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:layout_below="@+id/v_line_01"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/tv_auto_printing"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="186dp"
            android:gravity="center_vertical"
            android:text="检测完毕自动打印"
            android:textColor="#6b6b6b"
            android:textSize="18sp" />

        <Switch
            android:id="@+id/sw_print"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="224dp" />
    </LinearLayout>

    <View
        android:id="@+id/v_line_02"
        android:layout_width="440dp"
        android:layout_height="1dp"
        android:layout_below="@+id/ll_others"
        android:layout_marginLeft="186dp"
        android:background="#c0c0c0" />

    <TextView
        android:id="@+id/tv_upload_setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/v_line_02"
        android:layout_marginLeft="186dp"
        android:layout_marginTop="8dp"
        android:gravity="center_vertical"
        android:text="上传设置"
        android:textColor="#3d3d3d"
        android:textSize="18sp" />

    <View
        android:id="@+id/v_line_03"
        android:layout_width="440dp"
        android:layout_height="1dp"
        android:layout_below="@+id/tv_upload_setting"
        android:layout_marginLeft="186dp"
        android:layout_marginTop="5dp"
        android:background="#c0c0c0" />

    <LinearLayout
        android:id="@+id/ll_others02"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:layout_below="@+id/v_line_03"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/tv_upload"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="186dp"
            android:gravity="center_vertical"
            android:text="检测完毕自动上传"
            android:textColor="#6b6b6b"
            android:textSize="18sp" />
 <Switch
            android:id="@+id/sw_upload"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="224dp" />
        
    </LinearLayout>

    <View
        android:id="@+id/v_line_04"
        android:layout_width="440dp"
        android:layout_height="1dp"
        android:layout_below="@+id/ll_others02"
        android:layout_marginLeft="186dp"
        android:background="#c0c0c0" />

    <TextView
        android:id="@+id/tv_gps_locate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/v_line_04"
        android:layout_marginLeft="186dp"
        android:layout_marginTop="5dp"
        android:gravity="center_vertical"
        android:text="通道切换"
        android:textColor="#3d3d3d"
        android:textSize="18sp" />

    <View
        android:id="@+id/v_line_05"
        android:layout_width="440dp"
        android:layout_height="1dp"
        android:layout_below="@+id/tv_gps_locate"
        android:layout_marginLeft="186dp"
        android:layout_marginTop="5dp"
        android:background="#c0c0c0" />

    <LinearLayout
        android:id="@+id/ll_others03"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:layout_below="@+id/v_line_05"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/tv_gps"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="186dp"
            android:gravity="center_vertical"
            android:text="切换当前通道"
            android:textColor="#6b6b6b"
            android:textSize="18sp" />

        <Switch
            android:id="@+id/sw_channel_switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="260dp" />
       
    </LinearLayout>

    <View
        android:id="@+id/v_line_06"
        android:layout_width="440dp"
        android:layout_height="1dp"
        android:layout_below="@+id/ll_others03"
        android:layout_marginLeft="186dp"
        android:background="#c0c0c0" />

    <TextView
        android:id="@+id/tv_waizhi_print"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/v_line_06"
        android:layout_marginLeft="186dp"
        android:layout_marginTop="5dp"
        android:gravity="center_vertical"
        android:text="外置打印"
        android:textColor="#3d3d3d"
        android:textSize="18sp" />

    <View
        android:id="@+id/v_line_07"
        android:layout_width="440dp"
        android:layout_height="1dp"
        android:layout_below="@+id/tv_waizhi_print"
        android:layout_marginLeft="186dp"
        android:layout_marginTop="5dp"
        android:background="#c0c0c0" />

    <LinearLayout
        android:id="@+id/ll_others04"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:layout_below="@+id/v_line_07"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/tv_connect_print"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="186dp"
            android:gravity="center_vertical"
            android:text="是否连接外置打印机打印"
            android:textColor="#6b6b6b"
            android:textSize="18sp" />

         <Switch
            android:id="@+id/sw_link_print"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="170dp" />
    </LinearLayout>

    <View
        android:id="@+id/v_line_08"
        android:layout_width="440dp"
        android:layout_height="1dp"
        android:layout_below="@+id/ll_others04"
        android:layout_marginLeft="186dp"
        android:background="#c0c0c0" />

    <Button
        android:id="@+id/btn_return"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="562dp"
        android:layout_marginTop="402dp"
        android:background="@drawable/back"
        android:onClick="doClick" />

</RelativeLayout>